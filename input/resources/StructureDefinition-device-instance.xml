<?xml version="1.0" encoding="UTF-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://hl7.org/fhir ../input-cache/schemas-r5/fhir-single.xsd">
  <id value="device-instance"/>
  <url value="http://hl7.org/fhir/uv/pocd/StructureDefinition/device-instance"/>
  <name value="DeviceInstance"/>
  <title value="Device Instance extension"/>
  <status value="draft"/>
  <date value="2020-12-06"/>
  <publisher value="HL7 International - Health Care Devices Work Group"/>
  <description value="StructureDefinition that adds an instance number to Device."/>
  <fhirVersion value="4.0.1"/>
  <kind value="complex-type"/>
  <abstract value="false"/>
  <context>
    <type value="element"/>
    <expression value="Device"/>
  </context>
  <type value="Extension"/>
  <baseDefinition value="http://hl7.org/fhir/StructureDefinition/Extension"/>
  <derivation value="constraint"/>
  <mapping>
    <identity value="sdc"/>
    <uri value="urn:iso:std:iso:11073:10207"/>
    <name value="IEEE 11073-10207 SDC"/>
  </mapping>
  <differential>
    <element id="Extension">
      <path value="Extension"/>
      <short value="Instance number"/>
      <definition value="Instance number for multiple components of the same type"/>
    </element>
    <element id="Extension.url">
      <path value="Extension.url"/>
      <fixedUri value="http://hl7.org/fhir/uv/pocd/StructureDefinition/device-instance"/>
    </element>
    <element id="Extension.value[x]:valueUnsignedInt">
      <path value="Extension.valueUnsignedInt"/>
      <min value="1"/>
      <type>
        <code value="unsignedInt"/>
      </type>
    </element>
     <element id="Extension.extension:label">
      <path value="Extension.extension" />
      <sliceName value="label" />
      <short value="label" />
      <definition value="A human-readable label that describes the physical connector" />
    </element>
    <element id="Extension.extension.value[x]:valueString">
      <path value="Extension.extension.valueString" />
      <type>
        <code value="string" />
      </type>
       <mapping>
        <identity value="sdc"/>
        <map value="AbstractMetricState/PhysicalConnector/Label"/>
      </mapping>
    </element>
  </differential>
</StructureDefinition>